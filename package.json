{
  "name": "spext-xt-plugin",
  "displayName": "spext-xt1",
  "description": "spext-xt2",
  "version": "0.0.5",
  "publisher": "spext1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "pricing": "Free",
  "categories": [
    "Other"
  ],
  "extensionPack": [
    "ms-vscode.cpptools"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "SpecChecker",
      "properties": {
        "SpecChecker.path_to_executable": {
          "type": "string",
          "default": "",
          "description": "SpecChecker安装目录"
        }
      }
    },
    "commands": [
      {
        "command": "spext-xt.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "license": "https://github.com/sxei/vscode-plugin-demo/issues",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "pack": "node package.mjs",
    "pack-all": "node package-all.mjs"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "del": "^2.2.2",
    "eslint": "^8.41.0",
    "fs-extra": "^11.1.1",
    "fancy-log": "1.3.3",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "vsce": "^2.15.0",
    "node-fetch": "^2.7.0",
    "tar": "^6.1.15"
  }
}